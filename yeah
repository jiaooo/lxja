#include“iostream”
#include“algorithm”
using namespace std;
int main()
{
    int n,i,m=0,s,max=0,t,*a;
    //m标记元素，s统计该元素重数个数，max标记出现的最大重数，t表示该重数对应的元素
    scanf("%d",&n);
    printf("请输入6个数字：");
    a=new int[n];
 
    for(i=0;i<n;i++)
        scanf("%d",&a[i]);
    sort(a,a+n);
    for(i=0;i<n;i++)
    {
        if(m!=a[i])
            s=1;
        else
            s++;
        m=a[i];
        if(s>max)
        {
            t=m;
            max=s;
        }
    }
    printf("\n");
    printf("众数是：%d\n重数是：%d\n",t,max);
    return 0;    
}
